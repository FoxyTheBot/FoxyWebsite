{
    "version": 2,
    "builds": [
        {
            "src": "build/start.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/start.js"
        }
    ],
    "installCommand": "npx npm install",
    "outputDirectory": "build",
    "buildCommand": "npx npm run build"
}