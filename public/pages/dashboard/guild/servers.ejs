<html>

<head>
    <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7726590371480649"
        crossorigin="anonymous"></script>
    <link rel="stylesheet" href="../styles/header.css">
    <link rel="icon" href="../assets/images/foxycake.png">
    <title>Foxy | Servidores</title>
    <link rel="stylesheet" href="/styles/dashboard.css" type="text/css">
</head>

<body>
    <%- include('../../global/header') %>
    <main>
        <div class="servers">
            <% const mappedGuilds=guilds %>
                <% mappedGuilds.forEach(guild=> { %>
                    <div class="server">
                        <div class="server__icon">
                            <img src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>.png" alt=""
                                onerror="this.src='https://cdn.discordapp.com/attachments/1068525425963302936/1132377114755211284/Sem_titulo.png'">
                        </div>
                        <div class="server__info">
                            <h1>
                                <%= guild.name %>
                            </h1>
                        </div>
                        <div class="server__buttons">
                            <a href="/br/servers/<%= guild.id %>" class="server__button">Gerenciar</a>
                        </div>
                    </div>
                    <% }) %>
        </div>
        <div class="left-sidebar">
            <img class="sidebar-logo" src="/assets/images/logo-white.png">
            <div class="entries">              
                <hr class="separator">
                <h2 class="section-title">Opções do usuário</h2>

                <a href="/br/aboutme">
                    <div class="entry">
                        Alterar sobre mim
                    </div>
                </a>

                <a href="/br/daily">
                    <div class="entry">
                        Daily
                    </div>
                </a>

                <a href="/br/user/backgrounds">
                    <div class="entry">
                        Seus backgrounds
                    </div>
                </a>
                <hr class="separator">
                <h2 class="section-title">Lojinha da Foxy</h2>
                <a href="/br/store">
                    <div class="entry">
                        Backgrounds
                    </div>
                </a>
                <a href="/br/store">
                    <div class="entry">
                        Layouts de perfil
                    </div>
                </a>
                <a href="/br/store">
                    <div class="entry">
                        Decorações de avatar
                    </div>
                </a>

                <a href="/br/premium">
                    <div class="entry">
                        Premium e Cakes
                    </div>
                </a>
                <hr class="separator">
                <a href="/logout">
                    <div class="entry">
                        Deletar conta
                    </div>
                </a>
                <a href="/logout">
                    <div class="entry">
                        Sair
                    </div>
                </a>
            </div>
        </div>
    </main>
</body>
<script src="../js/HeaderMobile.js"></script>

</html>