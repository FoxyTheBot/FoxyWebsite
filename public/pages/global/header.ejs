<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles/header.css">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">
  <meta charset="UTF-8">
</head>
<header>
  <nav class="navigation-bar fixed" data-preload-persist="true" id="navigation-bar">
    <div class="left-entries">
      <button id="menu-open" class="mobile">
        <i class="fas fa-bars"></i>
      </button>
      <button id="menu-close" class="mobile" style="display: none;">
        <i class="fas fa-times"></i>
      </button>
      <div class="entry foxy-navbar-logo"><a href="/"><img src="/assets/images/Foxy.png" style="    font-size: 0px;
                    line-height: 0px;
                    width: 40px;
                    height: 40px;

                    position: absolute;
                    top: 3px;
                    border-radius: 100%;"></a></div>

      <div class="entry"><a href="/br/support">Suporte</a></div>
      <div class="entry"><a href="/br/support/terms">Termos de uso</a></div>
      <div class="entry"><a href="/upvote">Votar</a></div>
      <div class="entry"><a href="/br/premium">Premium</a></div>
      <div class="entry"><a href="/br/commands">Comandos</a></div>
      <div class="entry"><a href="/br/store">Loja de Itens</a></div>
      <div class="entry"><a href="/br/daily">Daily</a></div>
    </div>
    <div class="right-entries">
      <div class="entry">
        <div class="theme-switch-wrapper">
          <span class="theme-icon" id="theme-icon">
            <svg id="sun-icon" xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24" fill="none"
              stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <circle cx="12" cy="12" r="5"></circle>
              <line x1="12" y1="1" x2="12" y2="3"></line>
              <line x1="12" y1="21" x2="12" y2="23"></line>
              <line x1="4.22" y1="4.22" x2="5.64" y2="5.64"></line>
              <line x1="18.36" y1="18.36" x2="19.78" y2="19.78"></line>
              <line x1="1" y1="12" x2="3" y2="12"></line>
              <line x1="21" y1="12" x2="23" y2="12"></line>
              <line x1="4.22" y1="19.78" x2="5.64" y2="18.36"></line>
              <line x1="18.36" y1="5.64" x2="19.78" y2="4.22"></line>
            </svg>
            <svg id="moon-icon" xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24"
              fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
              style="display: none;">
              <path d="M21 12.79A9 9 0 1 1 11.21 3a7 7 0 0 0 9.79 9.79z"></path>
            </svg>
          </span>
        </div>
      </div>

      <% if (user) { %>
        <div class="entry">
          <a href="/br/dashboard" id="login-button">
            <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.png">
            <div></div>
            <span>
              <%= user.username.toUpperCase() %>
            </span>
          </a>
        </div>

        <% } else { %>
          <div class="entry">
            <a href="/br/dashboard" id="login-button">
              <div></div>
              <span>
                Entrar
              </span>
            </a>
          </div>
          <% } %>
    </div>
    </div>
  </nav>
</header>
<div id="mobile__menu" class="overlay">
  <div class="overlay__content">
    <a href="/br/support">Suporte</a>
    <a href="/br/support/terms">Termos de uso</a>
    <a href="/upvote">Votar</a>
    <a href="/br/premium">Premium</a>
    <a href="/br/commands">Comandos</a>
    <% if (user) { %>
      <a id="login-button" href="/br/dashboard">Dashboard</a>
      <% } else { %>
        <a id="login-button" href="/br/dashboard">Entrar</a>
        <% } %>
  </div>
</div>
<script src="/js/HeaderMobile.js"></script>